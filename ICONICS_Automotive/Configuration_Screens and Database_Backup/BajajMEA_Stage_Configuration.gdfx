              <x:XData><![CDATA[function stage_id_TextChanged(sender : System.Object, e : System.Windows.Controls.TextChangedEventArgs)
{
		var stage_id	   	    : System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("stage_id").ToDependencyObject();
		var stage_name   	    : System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("stage_name").ToDependencyObject();
		var stage_id_empty_label:Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("stage_id_empty_label");
		stage_id_empty_label.Visible = false
		var Reader : System.Data.SqlClient.SqlDataReader;

		var connString : String = "Data Source=" + DB_SERVER+ ";Initial Catalog=" + DB_NAME + DB_CONNECT_AUTHENTICATION;
  		var connection : System.Data.SqlClient.SqlConnection = new System.Data.SqlClient.SqlConnection(connString);
		try
		{
		connection.Open();
		var Data : System.Data.SqlClient.SqlCommand = new System.Data.SqlClient.SqlCommand("select stage_name  from Config_Stage where stage_id='"+stage_id.Text+"'", connection);
		Reader = Data.ExecuteReader();
		if(Reader.HasRows)
		{
			while(Reader.Read())
			{
				stage_name.Text = Reader("stage_name");
				
			 }                 
			Reader.Close();
		}
		else
		{
			stage_name.Clear();
		}
	}
	catch(ex : Exception) 
	{
		MessageBox.Show (ex.Message);	
	}
	Stage_ID();

}

