              <x:XData><![CDATA[
function ThisDisplay_AnimationStarted(sender : System.Object, e : System.EventArgs)
{

	var login_username					:	System.Windows.Controls.TextBox	=	ThisConfiguration.GetObjectByName("login_username").ToDependencyObject();	
	var login_user_password_control		:	Ico.Gwx.GwxWindowsFormsControl=ThisConfiguration.GetObjectByName("login_user_password");
	var login_user_password				: 	System.Windows.Forms.TextBox;	
	login_user_password					= 	System.Windows.Forms.TextBox(login_user_password_control.Control);	
	var error_message					:	Ico.Gwx.GwxLabel	=	ThisConfiguration.GetObjectByName("error_message");
	error_message.Visible				=	false;

	var login							:	String;
	login								=	ThisWindow.FwxClientWrapper.Read("@rgs64:Login.login.Value");
	
	if((login.Substring(0,login.IndexOf(",")))=="Logout")
	{		
			ThisWindow.FwxClientWrapper.WriteAsync("@rgs64:Login.UserName.Value","");
			ThisWindow.FwxClientWrapper.WriteAsync("@rgs64:Login.UserId.Value","");
			ThisWindow.FwxClientWrapper.WriteAsync("@rgs64:Login.Password.Value","");
			ThisWindow.FwxClientWrapper.WriteAsync("@rgs64:Login.login.Value","Login");
	}	
}

function ThisDisplay_AnimationStopped(sender : System.Object, e : System.EventArgs)
{
	var error_message		:	Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("error_message");
	error_message.Visible	=	false;	
	var login_username		:	System.Windows.Controls.TextBox=ThisConfiguration.GetObjectByName("login_username").ToDependencyObject();		
	login_username.Clear();
}
]]></x:XData>
            </script:ScriptCode>
