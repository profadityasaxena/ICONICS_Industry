            <script:ScriptCode Name="Login_Command" Type="JScriptNet">
              <x:XData><![CDATA[function Login_Command(sender : System.Object, cmdArgs : Ico.Gwx.CommandExecutionEventArgs){
		
		// [Creating Connection String]
		var connection_string : String = "Data Source="+DB_SERVER+"; Initial Catalog=" + DB_NAME + "; Password=" + DB_PASSWORD + "; User ID=" + DB_USERID + ";";
		// [Inline Test Statement] : Prints out connection_string : MessageBox.Show(connection_string);
		
		// [Opening connection with the database]
		var connection : System.Data.SqlClient.SqlConnection;
		connection = new System.Data.SqlClient.SqlConnection(connection_string);
		
		// [Declaration of Variables]
		var login_username 				: System.Windows.Controls.TextBox=ThisConfiguration.GetObjectByName("login_username").ToDependencyObject();
		
		// [Question] Ask Aditya Sharma for why this control variable has been created
		var login_user_password_control : Ico.Gwx.GwxWindowsFormsControl;
		login_user_password_control 	= Ico.Gwx.GwxWindowsFormsControl(ThisConfiguration.GetObjectByName("login_user_password"));
		login_user_password 			: System.Windows.Forms.TextBox;
		login_user_password				= System.Windows.Forms.TextBox(login_user_password_control.Control);
		var error_message 				: Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("error_message");
		
		var reader						: System.Data.SqlClient.SqlDataReader;
		var user_name;
		var user;
		var password;
		
		// [Opening SQL Connection]
		connection.Open();
		
		// [Creating the Query String]
		var query_string 	: System.Data.SqlClient.SqlCommand = new System.Data.SqlClient.SqlCommand("SELECT dbo.Config_User.* FROM dbo.Config_User WHERE user_name = '" + login_username.Text + "' and user_password='" + login_user_password.Text + "'", connection);
		
		// [Executing the Query]
		reader = query_string.ExecuteReader();	
		if(reader.HasRows == true)
		{
			if(reader.Read() == true)
			{
				//user_id			=	reader["user_id"];
				user_name		=	reader["user_name"];
				user_password	=	reader["user_password"];
				
			ThisWindow.FwxClientWrapper.WriteAsync("@rgs64:Login.UserName.Value",user_name);
			//ThisWindow.FwxClientWrapper.WriteAsync("@rgs64:Login.UserId.Value",user_id);
			ThisWindow.FwxClientWrapper.WriteAsync("@rgs64:Login.Password.Value",user_password);
			ThisWindow.FwxClientWrapper.WriteAsync("@rgs64:Login.login.Value","Logout");
	     	MessageBox.Show("Login Successful!!");
				ThisWindow.Close();
			}
			reader.Close();	
		}
	
		else
		{
			error_message.Visible = true;
		}
	}]]></x:XData>
            </script:ScriptCode>
