	var activity_enable	        	: System.Windows.Controls.CheckBox	= ThisConfiguration.GetObjectByName("activity_enable").ToDependencyObject();


	var line_empty_label         		: Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("line_empty_label");
	var sequence_empty_label         	: Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("sequence_empty_label");
	var check_point_empty_label         : Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("check_point_empty_label");
