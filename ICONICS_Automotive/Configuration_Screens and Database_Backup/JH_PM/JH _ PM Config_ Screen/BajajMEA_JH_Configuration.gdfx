    query_string += "@item      				= '"+items.Text+"',";
    query_string += "@machine_condition       	= '"+machine_condition.Text+"',";
    query_string += "@method      				= '"+method.Text+"',";
    query_string += "@frequency					= '"+frequency.Text+"',";
    query_string += "@duration      			= '"+duration.Text+"',";
    query_string += "@start_point       		= '"+start_point.Text+"',";
    //query_string += "@due_date      			    = '',";
	query_string += "@time       				= '"+time.Text+"',";
	query_string += "@inform_to_pm				= '"+inform_to_pm_enable_int+"',";
	query_string += "@activity_enable			= '"+activity_enable_int+"',";
	query_string += "@status					= 0,";
	query_string += "@created_by 			    = '"+created_by+"',";
    query_string += "@ActionOp 					= '"+ActionOp+"'";

    // [Validation Statement] This if-else statement checks if any of the fields have been left empty
    if (line_name.Text == ""|| sequence.Text == ""|| check_point.Text == ""|| stage_name.Text == ""||
		equipment_name.Text == ""|| c_l_i_rt.Text == ""|| related_to.Text == ""|| if_not_ok.Text==""||
		standard_condition.Text == ""|| items.Text == ""|| machine_condition.Text == ""|| 
		method.Text == ""|| frequency.Text == ""|| duration.Text == ""|| time.Text == "" ){

		 if(line_name.Text == "")
        {line_empty_label.Visible = true}
        else
        {line_empty_label.Visible = false}	   

		if(sequence.Text == "")
        {sequence_empty_label.Visible = true}
        else
        {sequence_empty_label.Visible = false}	

        if(check_point.Text == "")
        {check_point_empty_label.Visible = true}
        else
        {check_point_empty_label.Visible = false}
				
		if(stage_name.Text == "")
        {stage_empty_label.Visible = true}
        else
        {stage_empty_label.Visible = false}

		if(equipment_name.Text == "")
        {equipment_empty_label.Visible = true}
        else
        {equipment_empty_label.Visible = false}	

        if(c_l_i_rt.Text == "")
        {c_l_i_rt_empty_label.Visible = true}
        else
        {c_l_i_rt_empty_label.Visible = false}

		if(related_to.Text == "")
        {related_to_empty_label.Visible = true}
        else
        {related_to_empty_label.Visible = false}

        if(if_not_ok.Text == "")
        {if_not_ok_empty_label.Visible = true}
        else
        {if_not_ok_empty_label.Visible = false}		
				        
        if(standard_condition.Text == "")
        {standard_condition_empty_label.Visible = true}
        else
        {standard_condition_empty_label.Visible = false}

		if(items.Text == "")
        {items_empty_label.Visible = true}
        else
        {items_empty_label.Visible = false}

		if(machine_condition.Text == "")
        {machine_condition_empty_label.Visible = true}
        else
        {machine_condition_empty_label.Visible = false}
 
        if(method.Text == "")
        {method_empty_label.Visible = true}
        else
        {method_empty_label.Visible = false}
                        
        if(frequency.Text == "")
        {frequency_empty_label.Visible = true}
        else
        {frequency_empty_label.Visible = false}

        if(duration.Text == "")
		{duration_empty_label.Visible = true}
		else
		{duration_empty_label.Visible = false}

        if(time.Text == "")
        {time_empty_label.Visible = true}
        else
        {time_empty_label.Visible = false}

        

        } else {
            try {
                var reader : System.Data.SqlClient.SqlDataReader;	
                var stored_procedure_query : System.Data.SqlClient.SqlCommand = new System.Data.SqlClient.SqlCommand(query_string, connection);					
                reader = stored_procedure_query.ExecuteReader();
                
                while(reader.Read())
                {
                    response = reader["Response"];
                    MessageBox.Show(response);
                }
                reader.Close();
    
            } 
            catch(ex : Exception){
                MessageBox.Show (ex.Message);	
            }	
        }			
                        
        // [Closing connection with the database]
        Clear();
        connection.Close();
}]]></x:XData>
