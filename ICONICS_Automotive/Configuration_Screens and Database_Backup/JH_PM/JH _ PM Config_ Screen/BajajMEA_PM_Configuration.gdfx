            <script:ScriptCode Name="CLEAR" Type="JScriptNet">
              <x:XData><![CDATA[function Clear(sender : System.Object, cmdArgs : Ico.Gwx.CommandExecutionEventArgs){		
	
	// [Variable Declaration] Creating holder variables that will hold the user input values in the script
	var line_name       		: System.Windows.Controls.ComboBox  = ThisConfiguration.GetObjectByName("line_name").ToDependencyObject();
	var stage_name       		: System.Windows.Controls.ComboBox  = ThisConfiguration.GetObjectByName("stage_name").ToDependencyObject();
	var checkpoint	   	    	: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("checkpoint").ToDependencyObject();
	var assembly_name       	: System.Windows.Controls.ComboBox  = ThisConfiguration.GetObjectByName("assembly_name").ToDependencyObject();
	var sub_assembly_name      	: System.Windows.Controls.ComboBox  = ThisConfiguration.GetObjectByName("sub_assembly_name").ToDependencyObject();
	var frequency   	        : System.Windows.Controls.ComboBox	= ThisConfiguration.GetObjectByName("frequency").ToDependencyObject();
	var duration      			: System.Windows.Controls.ComboBox  = ThisConfiguration.GetObjectByName("duration").ToDependencyObject();
	var standard_condition   	: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("standard_condition").ToDependencyObject();
    var start_point   			:Ico.Gwx.GwxWindowsFormsControl    = Ico.Gwx.GwxWindowsFormsControl(ThisConfiguration.GetObjectByName("start_point"));
	var activity_enable	        	: System.Windows.Controls.CheckBox	= ThisConfiguration.GetObjectByName("activity_enable").ToDependencyObject();
    var due_date   					: Ico.Gwx.GwxWindowsFormsControl    = Ico.Gwx.GwxWindowsFormsControl(ThisConfiguration.GetObjectByName("due_date"));

	// [Array Reset] Collection arrays are reset here to default values
    
	
	//[Variable Reset] Variables are reset here to default values
	line_name.Items.Clear();
	stage_name.Items.Clear();
	assembly_name.Items.Clear();
	sub_assembly_name.Items.Clear();
	checkpoint.Clear();
	standard_condition.Clear();
    start_point.Text = "";
	activity_enable.IsChecked= false;
	due_date.Text = "";


	duration.SelectedIndex 				= 0;
	frequency.SelectedIndex				= 0;
		
	//[Inline Test Statemnt] MessageBox.Show('Clearing all fields');
}]]></x:XData>
