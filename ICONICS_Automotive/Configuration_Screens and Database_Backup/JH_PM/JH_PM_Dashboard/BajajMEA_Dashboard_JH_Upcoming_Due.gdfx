            <script:ScriptCode Name="CLEAR" Type="JScriptNet">
              <x:XData><![CDATA[function Clear(sender : System.Object, cmdArgs : Ico.Gwx.CommandExecutionEventArgs){		
	
	// [Variable Declaration] Creating holder variables that will hold the user input values in the script
 	var line_name       			: System.Windows.Controls.TextBox 	= ThisConfiguration.GetObjectByName("line_name").ToDependencyObject();
    var sequence        			: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("sequence").ToDependencyObject();
	var check_point        			: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("check_point").ToDependencyObject();
    var stage_name              	: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("stage_name").ToDependencyObject();
    var equipment_name       		: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("equipment_name").ToDependencyObject();
	var c_l_i_rt       				: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("c_l_i_rt").ToDependencyObject();
    var related_to        			: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("related_to").ToDependencyObject();
    var if_not_ok           		: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("if_not_ok").ToDependencyObject();   
	var standard_condition         	: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("standard_condition").ToDependencyObject();
    var items       				: System.Windows.Controls.TextBox  	= ThisConfiguration.GetObjectByName("items").ToDependencyObject();
    var machine_condition       	: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("machine_condition").ToDependencyObject();
	var method       				: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("method").ToDependencyObject();
	var frequency  					: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("frequency").ToDependencyObject();
    var duration       				: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("duration").ToDependencyObject();
    var start_point   				: Ico.Gwx.GwxWindowsFormsControl    = Ico.Gwx.GwxWindowsFormsControl(ThisConfiguration.GetObjectByName("start_point"));
    var due_date   					: Ico.Gwx.GwxWindowsFormsControl    = Ico.Gwx.GwxWindowsFormsControl(ThisConfiguration.GetObjectByName("due_date"));
	var time       					: System.Windows.Controls.TextBox   = ThisConfiguration.GetObjectByName("time").ToDependencyObject();
    var day_difference       		: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("day_difference").ToDependencyObject();
	var inform_to_pm       			: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("inform_to_pm").ToDependencyObject();
	var activity_enable				: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("activity_enable").ToDependencyObject();

	//var inform_to_pm_enable	        : System.Windows.Controls.CheckBox	= ThisConfiguration.GetObjectByName("inform_to_pm_enable").ToDependencyObject();
	//var activity_enable	        	: System.Windows.Controls.CheckBox	= ThisConfiguration.GetObjectByName("activity_enable").ToDependencyObject();
	
	// [Array Reset] Collection arrays are reset here to default values
	
	c_l_i_rt.Clear();
	related_to.Clear();
	machine_condition.Clear();
    method.Clear();
	frequency.Clear();
	duration.Clear();
    line_name.Clear();
	sequence.Clear();
	items.Clear();
    check_point.Clear();
