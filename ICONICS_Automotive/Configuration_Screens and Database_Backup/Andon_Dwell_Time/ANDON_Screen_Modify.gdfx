            <script:ScriptCode Name="update" Type="JScriptNet">
              <x:XData><![CDATA[function Update(sender : System.Object, cmdArgs : Ico.Gwx.CommandExecutionEventArgs){
		
		var connection_string : String = "Data Source="+DB_SERVER+"; Initial Catalog="+DB_NAME+"; Password="+DB_PASSWORD+"; User ID="+DB_USERID+";";
		// [Inline Test Statement] : Prints out connection_string : MessageBox.Show(connection_string);
		
		// [Opening connection with the database]
		var connection : System.Data.SqlClient.SqlConnection;
		connection = new System.Data.SqlClient.SqlConnection(connection_string);
		
		connection.Open();
            var screen_id       	    : System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("screen_id").ToDependencyObject();
			var screen_name             : System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("screen_name").ToDependencyObject();
					 	
			var updateCmd               : System.Data.SqlClient.SqlCommand  = new System.Data.SqlClient.SqlCommand(" Update Config_ANDON_Screen set screen_name = '"+screen_name.Text+"' where screen_id ='"+screen_id.Text+"'", connection);
			updateCmd.ExecuteNonQuery();  
			MessageBox.Show("updated Successfully");
	        // var cmd= "SELECT * FROM Fault_Configuration order by Fault_ID Asc"
			Clear();
			return;      
			}
					
					


