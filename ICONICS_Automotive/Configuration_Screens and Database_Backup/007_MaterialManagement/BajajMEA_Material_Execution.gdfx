            <script:ScriptCode Name="Dropdown_Material_Name" Type="JScriptNet">
              <x:XData><![CDATA[function Dropdown_material_name(sender : System.Object, e : System.EventArgs)
{
	    var connection_string : String = "Data Source="+DB_SERVER+"; Initial Catalog="+DB_NAME+"; Password="+DB_PASSWORD+"; User ID="+DB_USERID+";";
		// [Opening connection with the database]
		var connection : System.Data.SqlClient.SqlConnection;
		connection = new System.Data.SqlClient.SqlConnection(connection_string);
		
		connection.Open();

		// [Variable Declaration] Creating holder variables that will hold the user input values in the script
		var material_name                     	: System.Windows.Controls.ComboBox	= ThisConfiguration.GetObjectByName("material_name").ToDependencyObject();
	    var capacity                            : System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("capacity").ToDependencyObject();
        var threshold                           : System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("threshold").ToDependencyObject();
		var auxiliary                        	: System.Windows.Controls.ComboBox	= ThisConfiguration.GetObjectByName("auxiliary").ToDependencyObject();

		var material_name_empty_label  	: Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("material_name_empty_label");
	    material_name_empty_label.Visible  = false

		// [Array Reset] Collection arrays are reset here to default values
		material_name.Items.Clear();
			
		var DataReader : System.Data.SqlClient.SqlDataReader;
		var RetrieveDataCmd: System.Data.SqlClient.SqlCommand = new System.Data.SqlClient.SqlCommand("SELECT distinct material_name from MaterialManagement_Material_Config_Feeder WHERE auxiliary_id = (SELECT equipment_id from Config_Equipment where equipment_name = '"+auxiliary.Text+"')", connection);
		if (auxiliary.Text != ""){
		try{
			DataReader=RetrieveDataCmd.ExecuteReader();

			while(DataReader.Read())
			{
				material_name.Items.Add(DataReader["material_name"]);
			}
			DataReader.Close();
			
			var FetchCapacityCMD : System.Data.SqlClient.SqlCommand = new System.Data.SqlClient.SqlCommand("select capacity from MaterialManagement_Material_Config_Feeder where material_name = '"+material_name.Text+"'",connection);
		    capacity.Text = FetchCapacityCMD.ExecuteScalar();
		        
		    var FetchThresholdCMD : System.Data.SqlClient.SqlCommand = new System.Data.SqlClient.SqlCommand("select threshold from MaterialManagement_Material_Config_Feeder where material_name = '"+material_name.Text+"'",connection);
		    threshold.Text = FetchThresholdCMD.ExecuteScalar();

			connection.Close()
			
		}catch(ex:Exception){
			MessageBox.Show(ex);
		}
		} else {
            MessageBox.Show("You must select an auxiliary first");
        }


		
                



}
]]></x:XData>
            </script:ScriptCode>
