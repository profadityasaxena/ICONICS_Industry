            <script:ScriptCode Name="Dropdown_Location_Name" Type="JScriptNet">
              <x:XData><![CDATA[function Dropdown_Location_Name(sender : System.Object, e : System.EventArgs)
{
	
	    var connection_string : String = "Data Source="+DB_SERVER+"; Initial Catalog="+DB_NAME+"; Password="+DB_PASSWORD+"; User ID="+DB_USERID+";";
		// [Opening connection with the database]
		var connection : System.Data.SqlClient.SqlConnection;
		connection = new System.Data.SqlClient.SqlConnection(connection_string);
		
		connection.Open();

		
	// [Variable Declaration] Creating holder variables that will hold the user input values in the script
		var location_name        : System.Windows.Controls.ComboBox	= ThisConfiguration.GetObjectByName("location_name").ToDependencyObject();
		
		var location_name_empty_label      	: Ico.Gwx.GwxLabel         = ThisConfiguration.GetObjectByName("location_name_empty_label");
		location_name_empty_label.Visible       = false

	
	// [Array Reset] Collection arrays are reset here to default values
		location_name.Items.Clear();
	
			var DataReader : System.Data.SqlClient.SqlDataReader;
			var RetrieveDataCmd: System.Data.SqlClient.SqlCommand = new System.Data.SqlClient.SqlCommand("SELECT location_description from amr_location_config order by line_name ", connection);
			
			try{
				DataReader=RetrieveDataCmd.ExecuteReader();
	
				while(DataReader.Read())
				{
					location_name.Items.Add(DataReader["location_description"]);
				}
				DataReader.Close();
				connection.Close()
				
			}catch(ex:Exception){
				MessageBox.Show(ex);
			}
	}]]></x:XData>
            </script:ScriptCode>
            <script:ScriptCode Name="Dropdown_Rack_Code" Type="JScriptNet">
              <x:XData><![CDATA[function Dropdown_Rack_Code(sender : System.Object, e : System.EventArgs)
{
	 var connection_string : String = "Data Source="+DB_SERVER+"; Initial Catalog="+DB_NAME+"; Password="+DB_PASSWORD+"; User ID="+DB_USERID+";";
		// [Opening connection with the database]
		var connection : System.Data.SqlClient.SqlConnection;
		connection = new System.Data.SqlClient.SqlConnection(connection_string);
		
		connection.Open();

		
	// [Variable Declaration] Creating holder variables that will hold the user input values in the script
		var rack_code        : System.Windows.Controls.ComboBox	= ThisConfiguration.GetObjectByName("rack_code").ToDependencyObject();
		
