            <script:ScriptCode Name="Grid" Type="JScriptNet">
              <x:XData><![CDATA[function Grid(sender : System.Object, cmdArgs : Ico.Gwx.CommandExecutionEventArgs)
{
	
	// [Variable Declaration] Creating holder variables that will hold the user input values in the script
	var first_name       	: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("first_name").ToDependencyObject();
	var last_name      	    : System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("last_name").ToDependencyObject();
	var email       	    : System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("email").ToDependencyObject();	
	var mobile              : System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("mobile").ToDependencyObject();
	var department      	: System.Windows.Controls.ComboBox 	= ThisConfiguration.GetObjectByName("department").ToDependencyObject();
	var role            	: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("role").ToDependencyObject();
    
	
	var first_name_empty_label		 	 	: Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("first_name_empty_label");
	var last_name_empty_label    		 	: Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("last_name_empty_label");
    var email_empty_label		 	 		: Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("email_empty_label");
	var mobile_empty_label	 				: Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("mobile_empty_label");
	var role_empty_label	 		 		: Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("role_empty_label");
	var department_empty_label	 		    : Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("department_empty_label");
	var validate_user_mobile_phone_label	: Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("validate_user_mobile_phone_label");

	//[Grid Selection Code]
	var gdxViewer		: Ico.Gdx.GdxViewControl = Ico.Gdx.GdxViewControl(ThisConfiguration.GetObjectByName("GridWorXViewer1").ToDependencyObject());
	var tab_list 		: ItemsControl = ItemsControl(gdxViewer.Items[0]);
	var grid_list 		: Ico.Gdx.GdxGridView = Ico.Gdx.GdxGridView(tab_list.Items[0]);
	var Row_Selected 	: DataRow;
	Row_Selected = grid_list.GetHighlightedRow();
		
	if(grid_list.GetHighlightedRow() != null)
	{
			if (Row_Selected.Item[0] != String.IsNullOrEmpty){
				first_name.Text = Row_Selected.Item[1];
				last_name.Text = Row_Selected.Item[2];
				email.Text = Row_Selected.Item[3];
				mobile.Text = Row_Selected.Item[4];
				department.SelectedValue = Row_Selected.Item[5];
				department_empty_label.Visible           = false;
				role.Text = Row_Selected.Item[6];
			}
	}
}]]></x:XData>
