		    var last_name_empty_label    		 	: Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("last_name_empty_label");
            var email_empty_label		 	 		: Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("email_empty_label");
	        var mobile_empty_label	 				: Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("mobile_empty_label");
	        var role_empty_label	 		 		: Ico.Gwx.GwxLabel=ThisConfiguration.GetObjectByName("role_empty_label");
