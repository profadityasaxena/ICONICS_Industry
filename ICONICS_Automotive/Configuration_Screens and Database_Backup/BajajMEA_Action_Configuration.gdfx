			if ( shop_name.Text == ""|| line_name.Text == ""|| equipment_name.Text =="" || equipment_type.Text == ""|| action_description.Text ==""||stage_name.Text ==""||action_id.Text ==""||action_enabled ==""||created_by.Text=="" ){
