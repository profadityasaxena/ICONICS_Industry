            <script:ScriptCode Name="ADD" Type="JScriptNet">
              <x:XData><![CDATA[function Add(sender : System.Object, cmdArgs : Ico.Gwx.CommandExecutionEventArgs)
{
    var connection_string : String = "Data Source="+DB_SERVER+"; Initial Catalog="+DB_NAME+"; Password="+DB_PASSWORD+"; User ID="+DB_USERID+";";

    // [Opening connection with the database]
    var connection : System.Data.SqlClient.SqlConnection;
    connection = new System.Data.SqlClient.SqlConnection(connection_string);
    connection.Open();

    // [Operation Specification] Specifying the kind of operation that happens on pressing the button 
    var ActionOp = 'Create';

    // [Provisioning Security System]
    //var logged_in_user : System.Windows.Controls.TextBox = ThisConfiguration.GetObjectByName("logged_in_user").ToDependencyObject();
    //MessageBox.Show(logged_in_user.Text);

    // [Variable Declaration] Creating holder variables that will hold the user input values in the script
    var plant_name       			: System.Windows.Controls.ComboBox  = ThisConfiguration.GetObjectByName("plant_name").ToDependencyObject();
	var conveyor_name              	: System.Windows.Controls.ComboBox	= ThisConfiguration.GetObjectByName("conveyor_name").ToDependencyObject();
	var assembly_name       		: System.Windows.Controls.ComboBox  = ThisConfiguration.GetObjectByName("assembly_name").ToDependencyObject();
	var work_order_number        	: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("work_order_number").ToDependencyObject();
    var production_date   			: Ico.Gwx.GwxWindowsFormsControl    = Ico.Gwx.GwxWindowsFormsControl(ThisConfiguration.GetObjectByName("production_date"));
	var shift_id       				: System.Windows.Controls.ComboBox  = ThisConfiguration.GetObjectByName("shift_id").ToDependencyObject();
	var model_family_code      		: System.Windows.Controls.ComboBox  = ThisConfiguration.GetObjectByName("model_family_code").ToDependencyObject();	
	var model      					: System.Windows.Controls.ComboBox  = ThisConfiguration.GetObjectByName("model").ToDependencyObject();
	var sku_code           		    : System.Windows.Controls.ComboBox	= ThisConfiguration.GetObjectByName("sku_code").ToDependencyObject();
	var sequence_number     		: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("sequence_number").ToDependencyObject();   
	var color  						: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("color").ToDependencyObject();
	var planned_quantity  			: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("planned_quantity").ToDependencyObject();
	var kitting             		: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("kitting").ToDependencyObject();   
	var cclh_ccrh 					: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("cclh_ccrh").ToDependencyObject();
	var main  	            		: System.Windows.Controls.TextBox	= ThisConfiguration.GetObjectByName("main").ToDependencyObject();

    var plant_empty_label            	: Ico.Gwx.GwxLabel	=	ThisConfiguration.GetObjectByName("plant_empty_label");
	var conveyor_name_empty_label       : Ico.Gwx.GwxLabel	=	ThisConfiguration.GetObjectByName("conveyor_name_empty_label");
	var assembly_name_empty_label       : Ico.Gwx.GwxLabel	=	ThisConfiguration.GetObjectByName("assembly_name_empty_label");
	var work_order_empty_label  		: Ico.Gwx.GwxLabel	=	ThisConfiguration.GetObjectByName("work_order_empty_label");
	var shift_id_empty_label 			: Ico.Gwx.GwxLabel	=	ThisConfiguration.GetObjectByName("shift_id_empty_label");
	var model_family_empty_label 		: Ico.Gwx.GwxLabel	=	ThisConfiguration.GetObjectByName("model_family_empty_label");
	var model_name_empty_label			: Ico.Gwx.GwxLabel	=	ThisConfiguration.GetObjectByName("model_name_empty_label");
	var sku_code_empty_label  			: Ico.Gwx.GwxLabel	=	ThisConfiguration.GetObjectByName("sku_code_empty_label");
	var sequence_number_empty_label  	: Ico.Gwx.GwxLabel	=	ThisConfiguration.GetObjectByName("sequence_number_empty_label");
	var color_empty_label				: Ico.Gwx.GwxLabel	=	ThisConfiguration.GetObjectByName("color_empty_label");
    var planned_quantity_empty_label	: Ico.Gwx.GwxLabel	=	ThisConfiguration.GetObjectByName("planned_quantity_empty_label");
	
   // [Hardcoded Variable] These variables have to be replaced after taking inputs from Aditya Sharma
    //var created_by 		= logged_in_user.Text;

    //[Error & Exception Handling] Creating a variable for error and exception handling
    var response		: String;
    response="";

	var query_string : String = "Exec [dbo].[SP_OrderManagement_Master_Orders] ";

   //MessageBox.Show("[Debugger Mode] Print Query String : " + query_string);
	
    query_string += "@plant_name       			= '"+plant_name.Text+"',";
    query_string += "@conveyor_name 			= '"+conveyor_name.Text+"',";
    query_string += "@assembly_name 			= '"+assembly_name.Text+"',";
    query_string += "@work_order_number       	= '"+work_order_number.Text+"',";
    query_string += "@production_date 		   	= '"+production_date.Text+"',"
    query_string += "@shift_id 					= '"+shift_id.Text+"',"
    query_string += "@model_family_code 		= '"+model_family_code.Text+"',";
    query_string += "@model_description 		= '"+model.Text+"',";
    query_string += "@sku_code      			= '"+sku_code.Text+"',";
    query_string += "@sequence_number 			= '"+sequence_number.Text+"',";
    query_string += "@colour					= '"+color.Text+"',";
    query_string += "@planned_quantity			= '"+planned_quantity.Text+"',";
    query_string += "@ActionOp 					= '"+ActionOp+"'";

    // [Validation Statement] This if-else statement checks if any of the fields have been left empty
    if (plant_name.Text == ""|| conveyor_name.Text==""|| assembly_name.Text==""|| work_order_number.Text == ""|| production_date.Text==""|| shift_id.Text==""||  model_family_code.Text==""|| model.Text=="" ||sku_code.Text==""|| sequence_number.Text == ""|| color.Text==""|| planned_quantity.Text==""){
        if(plant_name.Text == "")
        {plant_empty_label.Visible = true}
        else
        {plant_empty_label.Visible = false}

        if(conveyor_name.Text == "")
        {conveyor_name_empty_label.Visible = true}
        else
        {conveyor_name_empty_label.Visible = false}

        if(assembly_name.Text == "")
        {assembly_name_empty_label.Visible = true}
        else
        {assembly_name_empty_label.Visible = false}

        if(work_order_number.Text == "")
        {work_order_empty_label.Visible = true}
        else
        {work_order_empty_label.Visible = false}
        
        if(shift_id.Text == "")
        {shift_id_empty_label.Visible = true}
        else
        {shift_id_empty_label.Visible = false}
        
        if(model_family_code.Text == "")
        {model_family_empty_label.Visible = true}
        else
        {model_family_empty_label.Visible = false}
        
        if(model.Text == "")
        {model_name_empty_label.Visible = true}
        else
        {model_name_empty_label.Visible = false}

        if(sku_code.Text == "")
        {sku_code_empty_label.Visible = true}
        else
        {sku_code_empty_label.Visible = false}
        
        if(sequence_number.Text == "")
        {sequence_number_empty_label.Visible = true}
        else
        {sequence_number_empty_label.Visible = false}
                        
        if(color.Text == "")
        {color_empty_label.Visible = true}
        else
        {color_empty_label.Visible = false}
		
		if(planned_quantity.Text == "")
        {planned_quantity_empty_label.Visible = true}
        else
        {planned_quantity_empty_label.Visible = false}
		
        } else {
            try {
                var reader : System.Data.SqlClient.SqlDataReader;	
                var stored_procedure_query : System.Data.SqlClient.SqlCommand = new System.Data.SqlClient.SqlCommand(query_string, connection);					
                reader = stored_procedure_query.ExecuteReader();
                
                while(reader.Read())
                {
                    response = reader["Response"];
                    MessageBox.Show(response);
                }
                reader.Close();
    
            } 
            catch(ex : Exception){
                MessageBox.Show (ex.Message);	
            }	
        }			
                        
        // [Closing connection with the database]
        Clear();
        connection.Close();
}]]></x:XData>
            </script:ScriptCode>
            <script:ScriptCode Name="Clear" Type="JScriptNet">
