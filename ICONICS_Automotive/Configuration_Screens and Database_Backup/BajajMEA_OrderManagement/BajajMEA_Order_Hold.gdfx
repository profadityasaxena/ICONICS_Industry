            <script:ScriptCode Name="holdCode" Type="JScriptNet">
              <x:XData><![CDATA[var connString : String = "Data Source=10.13.14.75; User ID=sa; Password=bajajadmin1; Initial Catalog=Bajaj; MultipleActiveResultSets=True"


function hold_btn(sender : System.Object, cmdArgs : Ico.Gwx.CommandExecutionEventArgs)
{

    var connection : System.Data.SqlClient.SqlConnection = new System.Data.SqlClient.SqlConnection(connString);	
	var from_Hold;
	var to_Hold;

	var HoldFromTxtCtl : Ico.Gwx.GwxWindowsFormsControl;
	var HoldFromTxt : System.Windows.Forms.TextBox;
	HoldFromTxtCtl = Ico.Gwx.GwxWindowsFormsControl(ThisConfiguration.GetObjectByName("HoldFrom"));
	HoldFromTxt = System.Windows.Forms.TextBox(HoldFromTxtCtl.Control);
	
	var HoldToTxtCtl : Ico.Gwx.GwxWindowsFormsControl;
	var HoldToTxt : System.Windows.Forms.TextBox;
	HoldToTxtCtl = Ico.Gwx.GwxWindowsFormsControl(ThisConfiguration.GetObjectByName("HoldTo"));
	HoldToTxt = System.Windows.Forms.TextBox(HoldToTxtCtl.Control);

	var holdReason: System.Windows.Controls.TextBox=ThisConfiguration.GetObjectByName("holdReason").ToDependencyObject();
		
	var sequenceComboBox : System.Windows.Controls.ComboBox = ThisConfiguration.GetObjectByName("sequence_hold").ToDependencyObject();	
	var variantComboBox : System.Windows.Controls.ComboBox = ThisConfiguration.GetObjectByName("variant_hold").ToDependencyObject();	
	var shiftComboBox : System.Windows.Controls.ComboBox = ThisConfiguration.GetObjectByName("shift_hold").ToDependencyObject();	
        
	var dateCtl : Ico.Gwx.GwxWindowsFormsControl;
	var dateTimePick : System.Windows.Forms.DateTimePicker;
	dateCtl = Ico.Gwx.GwxWindowsFormsControl(ThisConfiguration.GetObjectByName("DateTimePicker1"));
	dateTimePick= System.Windows.Forms.DateTimePicker(dateCtl.Control);
	var date=System.DateTime.Today;
	var dialog :System.Windows.MessageBoxResult
	var HoldRefreader : System.Data.SqlClient.SqlDataReader;
	
	var loginReader:System.Data.SqlClient.SqlDataReader;
	var userName: String
	var user: String
	var password: String

////////////////////checking for user authentication//////////////////////////////
	user=ThisWindow.FwxClientWrapper.Read("@reg:Login.UserId.Value");	
	password=ThisWindow.FwxClientWrapper.Read("@reg:Login.Password.Value");			
    
	connection.Open();
	var loginCmd: System.Data.SqlClient.SqlCommand = new System.Data.SqlClient.SqlCommand("SELECT User_ID,Password FROM Users where User_ID='" + user.Substring(0,user.IndexOf(",")) + "' and Password='" + password.Substring(0,password.IndexOf(",")) + "'", connection);
	loginReader=loginCmd.ExecuteReader();
	if(loginReader.HasRows==true)
	{
		loginReader.Close();
		    try{
			var fromHold_ref_command : System.Data.SqlClient.SqlCommand = new System.Data.SqlClient.SqlCommand("SELECT Ref_Seq_No FROM Order_Sequence WHERE Sequence_No = '" + HoldFromTxt.Text + "' and Prod_Date='" + date.ToString("yyyy-MM-dd") + "'",connection);
			var tohold_ref_command : System.Data.SqlClient.SqlCommand = new System.Data.SqlClient.SqlCommand("SELECT Ref_Seq_No FROM Order_Sequence WHERE Sequence_No = '" + HoldToTxt.Text + "' and Prod_Date='" + date.ToString("yyyy-MM-dd") + "'",connection);
			if(HoldFromTxt.Text != "" &&  HoldToTxt.Text != "")
			{
				
				RefHoldreader = fromHold_ref_command.ExecuteReader();
				 while (RefHoldreader.Read())
				{
					from_Hold = RefHoldreader["Ref_Seq_No"]; 
					//MessageBox.Show(from_Hold);                         										
				}
				RefHoldreader.Close();
				    
				RefHoldreader = tohold_ref_command.ExecuteReader();
				while (RefHoldreader.Read())
				{
					to_Hold = RefHoldreader["Ref_Seq_No"]; 
					//MessageBox.Show(to_Hold);                           										
				}
				RefHoldreader.Close();
			}
			
			var val_command : System.Data.SqlClient.SqlCommand= new System.Data.SqlClient.SqlCommand("UPDATE Order_Sequence SET Order_Status = 'Hold',Hold_Reason='" + holdReason.Text + "' WHERE Ref_Seq_No BETWEEN " + from_Hold + " AND " + to_Hold + " AND Order_Status = 'In Plan'" , connection); 
			var fromhold_command : System.Data.SqlClient.SqlCommand= new System.Data.SqlClient.SqlCommand("UPDATE Order_Sequence SET Order_Status = 'Hold' ,Hold_Reason='" + holdReason.Text + "' WHERE Sequence_No = '" + HoldFromTxt.Text + "' AND Order_Status = 'In Plan'" , connection); 
			var tohold_command : System.Data.SqlClient.SqlCommand= new System.Data.SqlClient.SqlCommand("UPDATE Order_Sequence SET Order_Status = 'Hold',Hold_Reason='" + holdReason.Text + "' WHERE Sequence_No = '" + HoldToTxt.Text + "' AND Order_Status = 'In Plan'" , connection); 
			
			dialog=MessageBox.Show("Do you want to Hold It ","Confirmation", MessageBoxButton.YesNo)
			
			if(HoldFromTxt.Text != "" ||  HoldToTxt.Text != "")
			{
				if(dialog==MessageBoxResult.Yes)
				{
					
					if(HoldFromTxt.Text != "" &&  HoldToTxt.Text != "" && (from_Hold <= to_Hold))
					{
						val_command.ExecuteNonQuery();
						MessageBox.Show("Updated Status Sucessfully");
					}
					else
					{
						MessageBox.Show("From Order Number Should Come First In The Sequence"+"\nOr"+"\nSelected sequence not from current date.")
					}
					if(HoldFromTxt.Text != "" &&  HoldToTxt.Text == "")
					{
						fromhold_command.ExecuteNonQuery();
						MessageBox.Show("Updated Status Sucessfully");
					}
					if(HoldFromTxt.Text == "" &&  HoldToTxt.Text != "")
					{
						tohold_command.ExecuteNonQuery();
						MessageBox.Show("Updated Status Sucessfully");
						
					}
					var insertDeleteLog:System.Data.SqlClient.SqlCommand = new System.Data.SqlClient.SqlCommand("Insert into Audit_Trial(User_ID, Screen_Name, Remark)values('" + user.Substring(0,user.IndexOf(",")) + "', 'Order Sequencing','Order from '+'" + HoldFromTxt.Text + "'+' to '+'" + HoldToTxt.Text + "' + ' on hold' )", connection);
					insertDeleteLog.ExecuteNonQuery();	
					HoldFromTxt.Clear();
					HoldToTxt.Clear();
					holdReason.Clear();
				}
			}
			else
			{
				MessageBox.Show("Please Enter OrderSequence Number");
			}
			
		        }
		catch(ex : Exception)
		{
			MessageBox.Show (ex.Message);	
		}
	}
	else
	{
		MessageBox.Show("Please Login First!!")
	}   
	connection.Close() 
}
]]></x:XData>
