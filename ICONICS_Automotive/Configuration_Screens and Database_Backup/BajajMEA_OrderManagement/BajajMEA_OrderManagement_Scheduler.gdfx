            <script:ScriptCode Name="Dropdown_Interval" Type="JScriptNet">
              <x:XData><![CDATA[function Dropdown_Interval(sender : System.Object, e : System.EventArgs)
{
	
		// [Variable Declaration] Creating holder variables that will hold the user input values in the script
		var enabled	                    : System.Windows.Controls.CheckBox	= ThisConfiguration.GetObjectByName("enabled").ToDependencyObject();
		var interval       			    : System.Windows.Controls.ComboBox	= ThisConfiguration.GetObjectByName("interval").ToDependencyObject();
		var interval_LB  : Ico.Gwx.GwxLabel                  = ThisConfiguration.GetObjectByName("interval_LB");
		interval_LB.Visible     = false
		
		// [Array Reset] Collection arrays are reset here to default values
