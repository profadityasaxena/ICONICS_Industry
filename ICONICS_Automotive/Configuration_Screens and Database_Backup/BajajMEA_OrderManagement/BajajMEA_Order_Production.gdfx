    if ( plant.Text == ""|| assembly.Text =="" || conveyor.Text == ""|| production_date.Text =="" || production_shift.Text =="" || seq_no.Text==""|| model.Text==""|| work_order.Text=="" || sku.Text==""|| colour.Text=="" || sku_description.Text=="" || plan_quantity.Text==""){
