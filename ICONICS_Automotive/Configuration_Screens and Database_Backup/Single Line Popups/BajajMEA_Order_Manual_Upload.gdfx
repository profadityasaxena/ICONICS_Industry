        if(sku.Text == "")
        {sku_empty_label.Visible = true}
        else					
        {sku_empty_label.Visible = false}
        
        if(seq_no.Text == "")
        {seq_no_empty_label.Visible = true}
        else					
        {seq_no_empty_label.Visible = false}
        
        if(work_order.Text == "")
        {work_order_empty_label.Visible = true}
